+import {Component, OnInit} from '@angular/core';
+import {Observable} from 'rxjs/Observable';
+import {EventSourcePolyfill} from 'ng-event-source';
+import {NgZone} from '@angular/core';
+
+@Component({
+  selector: 'app-root',
+  templateUrl: './app.component.html',
+  styleUrls: ['./app.component.css']
+})
+export class AppComponent implements OnInit {
+  title = 'Movie App';
+  movieList: Movie[] = [];
+  zone: NgZone;
+  ngOnInit(): void {
+    this.getMovies();
+  }
+
+  getMoviesService() {
+    return Observable.create(observer => {
+      const evtSource = new EventSourcePolyfill('http://localhost:8080/api/', {
+        heartbeatTimeout: 1000,
+        connectionTimeout: 1000,
+        errorOnTimeout: false
+      });
+
+      evtSource.onmessage = event => {
+        const movie = JSON.parse(event.data);
+        if (movie.title === 'A bore Movie') {
+          evtSource.close();
+          observer.complete();
+          return;
+        }
+        observer.next(movie);
+      };
+      evtSource.onerror = event => {
+        /*observer.error(event);
+        evtSource.close();
+        observer.complete();*/
+      };
+    });
+  }
+
+  getMovies() {
+    this.getMoviesService().subscribe(
+      movie => this.showMovieList(movie),
+      error => console.log('Gotten error in observable stream ' + error),
+      () => console.log('Observable stream got closed.')
+    );
+  }
+
+  showMovieList(movie: Movie) {
+    this.movieList.push(movie);
+    console.log(movie.title);
+  }
+
+}
+
+export class Movie {
+  constructor(
+    public id: number,
+    public title: string) {
+  }
+}
